{
  "functions": {
    "pages/api/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/pdf/(.*)",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=31536000"
        },
        {
          "key": "Content-Type",
          "value": "application/pdf"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/pdf/:path*",
      "destination": "/pdf/:path*"
    }
  ]
}